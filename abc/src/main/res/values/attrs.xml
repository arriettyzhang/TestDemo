<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleSeekBar">
        <attr name="circle_line_width" format="dimension"/>
        <attr name="seekbar_bg_color" format="color"/>
        <attr name="thumb_radius" format="dimension"/>
        <attr name="thumb_color" format="color"/>
        <attr name="start_angle" format="integer"/>
        <attr name="max_angle" format="integer"/>
        <attr name="current_progress" format="integer"/>
        <attr name="max_progress" format="integer"/>
        <attr name="thumb_drawable" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CircleSeekBar2">
        <attr name="circle_line_width2" format="dimension"/>
        <attr name="seekbar_bg_color2" format="color"/>
        <attr name="thumb_radius2" format="dimension"/>
        <attr name="thumb_drawable2" format="reference"/>
        <attr name="start_angle2" format="integer"/>
        <attr name="max_angle2" format="integer"/>
        <attr name="current_progress2" format="integer"/>
        <attr name="max_progress2" format="integer"/>
    </declare-styleable>
    <declare-styleable name="TurntableView">
        <attr name="bg_color" format="color"/>
        <attr name="text_color" format="color"/>
        <attr name="text_size" format="dimension"/>
        <attr name="region_color1" format="color"/>
        <attr name="region_color2" format="color"/>
        <attr name="region_color3" format="color"/>
        <attr name="divide_line_color" format="color"/>
        <attr name="divide_line_width" format="dimension"/>
        <attr name="circle_stroke_dimen" format="dimension"/>
        <attr name="btn_drawable" format="reference"/>
        <attr name="litter_bit_width" format="dimension"/>
    </declare-styleable>
</resources>